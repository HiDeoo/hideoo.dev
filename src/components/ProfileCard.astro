---
import { fetchGitHubInfos } from '../libs/github'

import Card from './Card.astro'
import CardRow from './CardRow.astro'

const gitHubInfos = await fetchGitHubInfos()
---

<Card>
  <CardRow>{gitHubInfos.login}</CardRow>
  <CardRow>Card block 2</CardRow>
  <CardRow>Card block 3</CardRow>
</Card>
